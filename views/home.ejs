<%- include("partials/header"); -%>
<h1>Home</h1>
<p><%=Startingcontent%></p>


<% posts.forEach(function(element){          %>
<h1><%= element.title;                       %></h1>
<p>
    <%= element.content.substring(0,100) + "...";%>
    <a href="/posts/<%=element.title%>">Read More</a>
</p>
<% }) %>
<form action="/compose">
<button type="submit" class="btn btn-secondary btn-lg btn-block button" >Compose</button>
</form>
<%- include("partials/footer"); -%>